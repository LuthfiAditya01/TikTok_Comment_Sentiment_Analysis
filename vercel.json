{
  "version": 2,
  "builds": [
    {
      "src": "./app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./app.py"
    }
  ],
  "functions": {
    "app.py": {
      "excludeFiles": "**/{requirements.txt,*.csv,venv/**,__pycache__/**,.git/**,*.pyc,report_html.py,makefile,*.md}"
    }
  }
} 